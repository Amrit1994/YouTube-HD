{
  "name": "HD quality for YouTubeâ„¢ (1080p, 1440p, 2160p)",
  "short_name": "iyutubehd",
  "description": "Watch YouTube in the full-HD quality or the highest available one",
  "author": "James Fray",
  "version": "0.2.2",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
